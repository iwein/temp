{
  "static-pages": {
    "options": {
      "metadata": {
        "api_url": "<%= env.api_url %>"
      },
      "plugins": {
        "metalsmith-templates": {
          "engine": "handlebars",
          "directory": "apps/index",
          "partials": {
            "footer": "../common/footer"
          }
        }
      }
    },
    "src": "<%= files.index.pages %>",
    "dest": "<%= files.index.out.pages %>"
  }
}
