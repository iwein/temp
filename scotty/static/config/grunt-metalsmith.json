{
  "static-pages": {
    "options": {
      "metadata": {
        "api_url": "<%= env.api_url %>"
      },
      "plugins": {
        "metalsmith-templates": {
          "engine": "handlebars",
          "directory": "apps/index"
        }
      }
    },
    "src": "<%= files.index.pages_folder %>",
    "dest": "<%= files.index.out.pages_folder %>"
  }
}
